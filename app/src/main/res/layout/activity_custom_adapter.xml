<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/background_image"
        tools:context=".CustomAdapter">
    <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/ProductImageAdapter"
            android:layout_width="99dp"
            android:layout_height="105dp"
            android:scaleType="centerCrop"
            android:src="@drawable/ic_baseline_image_200"
            app:civ_border_color="@color/colorPrimary"
            app:civ_border_width="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.016"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.223"/>

    <ImageButton
            android:id="@+id/delete_button"
            android:layout_width="37dp"
            android:layout_height="40dp"
            android:layout_marginTop="128dp"
            android:contentDescription="@string/todo1"
            android:minHeight="48dp"
            android:src="@drawable/ic_baseline_delete_24"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.093"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="SpeakableTextPresentCheck,DuplicateSpeakableTextCheck"/>
    <ImageButton
            android:id="@+id/addFeedBack"
            android:layout_width="37dp"
            android:layout_height="40dp"
            android:layout_marginTop="128dp"
            android:contentDescription="@string/todo1"
            android:minHeight="48dp"
            android:src="@drawable/ic_action_add_feedback"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.093"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="SpeakableTextPresentCheck,DuplicateSpeakableTextCheck"/>

    <TextView
            android:id="@+id/nameTextview"
            android:layout_width="96dp"
            android:layout_height="31dp"
            android:text="@string/name"
            android:textColor="@color/colorWhite"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.434"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.102"/>

    <TextView
            android:id="@+id/PricetextView4"
            android:layout_width="87dp"
            android:layout_height="31dp"
            android:text="@string/price"
            android:textColor="@color/colorWhite"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.422"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.503"/>

    <TextView
            android:id="@+id/qntitytextView5"
            android:layout_width="103dp"
            android:layout_height="33dp"
            android:text="@string/quantity"
            android:textColor="@color/colorWhite"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.444"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.948"/>

    <EditText
            android:id="@+id/nameeditText2"
            android:layout_width="130dp"
            android:layout_height="48dp"
            android:autofillHints=""
            android:ems="10"
            android:enabled="false"
            android:inputType="textPersonName"
            android:minHeight="48dp"
            android:textColor="@color/colorWhite"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.889"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.029"
            tools:ignore="LabelFor,SpeakableTextPresentCheck"/>

    <EditText
            android:id="@+id/priceeditText3"
            android:layout_width="130dp"
            android:layout_height="48dp"
            android:autofillHints=""
            android:ems="10"
            android:enabled="false"
            android:inputType="textPersonName"
            android:textColor="@color/colorWhite"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.889"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="LabelFor,SpeakableTextPresentCheck,TouchTargetSizeCheck"/>

    <EditText

            android:id="@+id/qquantityeditText4"
            android:layout_width="80dp"
            android:layout_height="48dp"
            android:layout_marginTop="100dp"
            android:autofillHints=""
            android:ems="10"
            android:enabled="false"
            android:inputType="number"
            android:textColor="@color/colorWhite"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.505"
            app:layout_constraintStart_toEndOf="@+id/qntitytextView5"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0"
            tools:ignore="LabelFor,SpeakableTextPresentCheck"/>

    <ImageView

            android:id="@+id/imageViewMinus"
            android:layout_width="45dp"
            android:layout_height="40dp"
            android:contentDescription="@string/todo"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.655"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0"
            app:srcCompat="@android:drawable/btn_minus"
            tools:ignore="ImageContrastCheck"/>

    <ImageView
            android:id="@+id/imageViewPlus"
            android:layout_width="45dp"
            android:layout_height="39dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0"
            app:srcCompat="@android:drawable/btn_plus"
            tools:ignore="ContentDescription,ImageContrastCheck"/>

</androidx.constraintlayout.widget.ConstraintLayout>